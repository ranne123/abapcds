// Extend the extension include view E_SalesDocumentItemBasic 
// with the Priority field and its foreign key association
@AbapCatalog.sqlViewAppendName: 'ZXESDIBPRIO'

extend view E_SalesDocumentItemBasic with ZX_E_SalesDocItemBasic_Prio

  association [0..1] to Z_Priority as _ZZPriority on $projection.ZZPriority = _ZZPriority.ZZPriority

{
  @ObjectModel.foreignKey.association: '_ZZPriority'
  Persistence.zzprio as ZZPriority,
  _ZZPriority
}
